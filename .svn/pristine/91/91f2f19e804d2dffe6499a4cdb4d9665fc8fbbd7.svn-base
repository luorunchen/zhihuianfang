body
{
    font-family:'Microsoft YaHei';

}
::-webkit-scrollbar-track
{
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	background-color: #49768d;
}

::-webkit-scrollbar
{
	width: 10px;
    height:10px;
	background-color: #55a1c5;
}

::-webkit-scrollbar-thumb
{
	background-color: #55a1c5;
}
.SElec_ElecComDev{
  position: absolute;
}
.SElec_ElecComDev .modal-dialog{
  margin-right: 1390px;
}
.contentBox {
    background: url('../images/bodybg.jpg') no-repeat;
    background-size:100% 100%;
}
.HeadBox {
    position:absolute;
    left:0px;
    top:0px;
    right:0px;
    height:70px;
    overflow:hidden;
    background: url('../images/titlebg.png') repeat-x;
}

.HeadBox .WebTitle
{
    width:100%;
    text-align:center;
    background-size:contain;
    font-size:30px;
    color:#fff;
    line-height:70px;

}

.HeadBox .WebTitle img
{
    height:60px;
    margin-top:10px;
}
.HeadBox .userTool{
   
}
.HeadBox .userTool a
{
   margin-left:10px;
   overflow:hidden;
   display:block;
   float:left;
   border-radius:10px;
   padding-left:10px;
   padding-right:10px;
    padding-top:5px;
   padding-bottom:5px;
}
.HeadBox .userTool a:hover
{
  background-color:#03469d;
 
}
.HeadBox .userTool img{
    width:40px;
    height:40px;
}
.HeadBox .leftMenu
{
   position:absolute;
    left:20px;
    top:10px;

}
.HeadBox .rightMenu
{
    position:absolute;
    top:10px;
    right:20px;
}

.NavBodyBox
{
    position:absolute;
    left:0px;
    right:0px;
    top:70px;
    bottom:0px;
    overflow:auto;
    color:#fff;
}

.NavBodyBox  .NavTable
{
    width:100%;
    height:100%;
}


.NavBodyBox  .NavCell
{
    text-align:center;
    vertical-align:central;
    height:100%;
    width:100%;
}

.NavBodyBox .NavMenuBox
{
    /*margin-left:100px;*/
    height:95%;
    max-height:680px;
   position:relative;
   /*width: 100%;*/
}

.NavBodyBox .LeftNavBox
{
     /*position:absolute;*/
     /*left: 50%;*/
     /*top:0px;*/
    /*margin-left:-645px ;*/
     /*bottom:0px;*/
     margin: 0 auto;
}

.NavBodyBox .NavTitle
{
    font-size:30px;
    text-align:left;
    height:90px;
    line-height:100px;
}
.NavBodyBox .left, .NavBodyBox .right
{
    height:600px;
   
}
.NavBodyBox .left{
  width: 11%;
   float: left;
}

.NavBodyBox .right{
  width: 28%;
  float: right;
}

.NavBodyBox .MenuRow
{
    height:30%;
}
.NavBodyBox .MenuItem
{
   height:100%;
   float:left;
   padding-right:15px;
   padding-bottom:15px;
   position:relative;
  
}

.NavBodyBox  .nowValid
{

    color:#b7b7b7;
    
}


.NavBodyBox .MenuItem1
{
   width:195px;
   height:100%;
    position:relative;
   cursor:pointer;
   border-radius: 5px;
   box-shadow: 3px 4px 8px #292929;
}

.MenuItem1 .MenuTitle
{
    font-size:16px;
    line-height:20px;
    text-align:center;
    position:absolute;
    left:0px;
    right:0px;
    bottom:10px;
    padding-left:10px;
    padding-right:10px;
  
}

.MenuItem1 .MenuContent
{
    position:absolute;
    left:0px;
    right:0px;
    top:0px;
    bottom:40px;
}
.MenuItem1 .MenuPic {
    background: url('../images/pic.png') no-repeat center;
    background-size:85px 85px;
    width:100%;
    height:100%;
}


.NavBodyBox .MenuItem2
{
   width:408px;
   height:100%;
   position:relative;
      cursor:pointer;
    border-radius: 5px;
    box-shadow: 3px 4px 8px #292929;
}



.MenuItem2 .MenuTitle
{
    font-size:18px;
    line-height:45px;
     padding-left:20px;
     text-align:left;
}

.MenuItem2 .MenuContent
{
    position:absolute;
    left:0px;
    right:0px;
    top:45px;
    bottom:0px;
}

.MenuItem2 .MenuText
{
    font-size:15px;
    line-height:25px;
    height:100%;
    width:100%;
    text-align:left;
    padding-left:20px;
    padding-right:20px;
}


.NavBodyBox .MenuItem1:hover,.MenuItem2:hover
{
   box-shadow:0px 0px 20px #FFF;
   opacity:0.8;
}
  
.NewsBox
{
    width:350px;
    height:75%;
    overflow-y:auto;
}


.media
{
    cursor:pointer;
   

}

.media img
{
    width:64px;
    height:64px;
} 


.media  .media-body
{
    text-align:left;
    font-size:14px;
    text-indent:2em;
}

.media  .media-heading
{
    font-size:16px;
    padding-bottom:5px;
    text-indent: 0em;
}

.blackFrame{


}

.blackFrame .modal-content
{

     background-color:#1d2528;
     color:#fff;
}
.blackFrame .modal-header
{
    border-bottom:solid 1px #75c5f0;
}

.blackFrame .modal-header span
{
    color:#fff;
}

@media screen and (max-width:2000px){
  .NavBodyBox .left{width: 26%}
  .NavBodyBox .right{width: 53%;}
}

@media screen and (max-width:1640px){
  .NavBodyBox .left{width: 30%}
  .NavBodyBox .right{width: 49%;}
}

@media screen and (max-width:1500px){
  .NavBodyBox .right{width: 50%;}
  .NavBodyBox .left{width: 33%}

}

@media screen and (max-width:1140px){
  .NavBodyBox .left{width: 43%}
  .NavBodyBox .right{width: 44%;}
}