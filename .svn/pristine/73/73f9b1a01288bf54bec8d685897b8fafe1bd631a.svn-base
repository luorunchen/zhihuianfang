.ToolBarBox {
    display: none;
}

.reportBody {
    top: 5px;
    overflow-y:auto;
    overflow-x:hidden;
}




.InforRow
{
    width:100%;
    overflow:hidden;
}

.InforBox
{
    border:solid 1px #ccc;
    margin:10px;
    float:left;
    border-radius:10px;
    padding:10px;
    position:relative;
}
.inforTitle
{
    width:100%;
    height:50px;
    line-height:50px;
    font-size:18px;
    padding-left:25px;
}

.inforLine
{
    /*height:25px;*/
    height:22px;
    padding-bottom:10px;
}



.inforLabel
{
    width:90px;
    float:left;
    /*text-align:right;*/
}
.inforValue
{
    margin-left:95px;
    color:#0000ff;
}


.LeftBox {
    border-radius: 5px;
    width: 180px;
    position:relative;
    float:left;
    display:table;
}

.RightBox
{
    margin-left:150px;
    height:100%;
    position:relative;
}

.chartBox
{
    background-color:transparent;
    border:none;
    bottom:0px;
}

.topBox {
    position:relative;
    top:300px;
    left: 180px;
}

.bottomBox {
    left: 180px;
    position:relative;
}

.redBg {
    color: #F00;
}

.safeBox
{
    height:180px;
    width:180px;
    margin-left:auto;
    margin-right:auto;
    line-height:180px;
    text-align:center;
    font-size:30px;
    border-radius:90px;
    background-color:#50B432;
    color:#fff;
    border:solid 1px #fff;

}
.huiBox
{
    height:180px;
    width:180px;
    margin-left:auto;
    margin-right:auto;
    line-height:180px;
    text-align:center;
    font-size:30px;
    border-radius:90px;
    background-color:#949393;
    color:#fff;
    border:solid 1px #fff;

}
.alarmBox
{
    height:180px;
    width:180px;
    margin-left:auto;
    margin-right:auto;
    line-height:180px;
    text-align:center;
    font-size:25px;
    border-radius:90px;
    background-color:red;
    color:#fff;
    border:solid 1px #fff;

}
.sageBoxCirred{
    background-color:red;
    color:#fff;
    border:solid 1px #fff;
    height:180px;
    width:180px;
    margin-left:auto;
    margin-right:auto;
    line-height:180px;
    text-align:center;
    font-size:30px;
    border-radius:90px;
}
.sageBoxGrey{
    background-color:#949393;
    color:#fff;
    border:solid 1px #fff;
    height:180px;
    width:180px;
    margin-left:auto;
    margin-right:auto;
    line-height:180px;
    text-align:center;
    font-size:30px;
    border-radius:90px;
}
.InforCurrBox
{
    display:table-cell;
    vertical-align:middle;
}

.DataItem{
    cursor:pointer;
}
.UnDataItem 
{
    color:#bbb;
}
.UnDataItem  .inforValue
{
    color:#bbb;
}

