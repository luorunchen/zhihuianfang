html
{
    width:100%;
    height:100%;
    margin:0px;
    padding:0px;
}
/*基础样式*/
body {
  
    width:100%;
    height:100%;
    font-size:13px;
    overflow:hidden;
    font-family:'Microsoft YaHei';
    scrollbar-face-color: #b46868;
      background-color:#1d2528;
}
a {
  cursor: pointer;
}
a:hover,
a:focus {
  text-decoration: none;
}

 ::-webkit-scrollbar-track
{
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	background-color: #49768d;
}

::-webkit-scrollbar
{
	width: 10px;
    height:10px;
	background-color: #55a1c5;
}

::-webkit-scrollbar-thumb
{
	background-color: #55a1c5;
}

.form-control{

   background-color:#262626;
   border:solid 1px #248ef4;
   color:#fff;
}

.form-control[disabled], .form-control[readonly], fieldset[disabled] .form-control {
    opacity:0.5;
    background-color:#466a7c;
}

.bodyBox
{
    position:relative;
    height:100%;
    width:100%;
    margin-left:auto;
    margin-right:auto;
}
.MapBox
{
    position:absolute;
    top:0px;
    left:0px;
    right:0px;
    bottom:00px;
    width:auto;
    height:auto;
}

.leftBox
{
    position:absolute;
    top:155px;
    left:60px;
    width:280px;
    bottom:0px;
    overflow-y:auto;
}


.HeadBox {
    position:absolute;
    left:0px;
    top:0px;
    right:0px;
    height:80px;
    overflow:hidden;
    background: url(../images/titlebg2.png) repeat-x;
}

.HeadBox .WebTitle
{
    width:100%;
    margin-top:5px;
    text-align:center;
    background-size:contain;
}

.HeadBox .WebTitle img
{
    height:60px;
    margin-top:10px;
}
.HeadBox .userTool{
    position:absolute;
    left:20px;
    top:20px;
}
.HeadBox .userTool a
{
   margin-left:10px;
   overflow:hidden;
   display:block;
   float:left;
   border-radius:10px;
   padding-left:10px;
   padding-right:10px;
    padding-top:5px;
   padding-bottom:5px;
}
.HeadBox .userTool a:hover
{
  background-color:#49768d;
 
}
.HeadBox .userTool img{
    width:40px;
    height:40px;
}


.HeadBox .rightMenu
{
    position:absolute;
    padding-right:110px;
    top:20px;
    right:10px;
    color:#ffffff;
    font-size:30px;
}
.HeadBox .forwardImg
{
    width:60px;
    height:60px;
    background: url('..images/goForward.png') no-repeat center center;
    background-size:100% 100%;
    cursor:pointer;
    position:absolute;
    right:20px;
    top:-5px;
}


.numBox {
    color: #fff;
    background: url(../images/kjbg.png);
    border: solid 1px #fff;
    box-shadow:inset 0px 0px 30px #fff;
    border-radius: 7px;
    padding-bottom: 10px;
    padding-left: 10px;
    padding-right:10px;
  
}
.numBox .numTitle
{

    font-size:16px;
    height:40px;
    line-height:40px;

}
.numBox .numContent
{
    width:100%;
    height:40px;
}
.numBox .numContent .numCell
{
    float:left;
    width:35px;
    text-align:center;
    font-size:20px;
    line-height:35px;
    background-color:#248ef4;
    margin-left:5px;
} 
.numBox .numItem
{
    height:40px;
    line-height:40px;
    border: solid 1px #248ef4;
    margin-top:10px;
    padding-left:15px;
    padding-right:15px;
    border-radius:5px;
    cursor:pointer;

}

.numBox .numItem{
   background: url(../images/m_l.png) no-repeat right;
   background-size: 4%;
}
.numBox .numItem .valueCell
{
    float:right;
    height:40px;
    line-height:40px;
}


/*查询窗口*/
.queryBox
{
    position:absolute;
    top:155px;
    right:10px;
    width:300px;
    color: #fff;
    background: url(../images/kjbg.png);
    border: solid 1px #fff;
    box-shadow:inset 0px 0px 30px #fff;
    border-radius: 7px;
    padding-bottom:5px;
}
.queryBox .queryToolBar
{
    height:55px;
    padding:10px;
    line-height:40px;
    position:relative;
}

.queryBox .queryToolBar .queryText{
    width:220px;
    display:inline;
}
    .queryBox .queryToolBar .queryBut {
        position:absolute;
        right:10px;
        top:13px;
    }

   .queryBox   .queryResult
   {
        max-height:400px;
        overflow-y:auto;
   }
     .queryBox   .queryResult .resultItem
     {
          border-top: solid 1px #5dacd3;
          cursor:pointer;
     }
      .queryBox   .queryResult .resultItem:hover
     {
         background-color:#057681;
         color:#01e3e7;

     }
     .queryBox   .queryResult .resultTitle
     {
        font-size:14px;
        padding-top:5px;
        padding-bottom:5px;
        padding-left:20px;
     }
       .queryBox   .queryResult .resultText
     {
        font-size:12px;
        padding-bottom:5px;
        padding-left:20px;
     }
     

       /*报警图表*/
       .chartBox
       {

            position:absolute;
            bottom:10px;
            right:10px;
            width:40px;
            height:250px;
            color: #fff;
            background: url(../images/kjbg.png);
            border: solid 1px #fff;
            box-shadow:inset 0px 0px 30px #fff;
            border-radius: 7px;
       }
       .chartBody
       {
           display:none;
           width:100%;
           height:100%;
           position:relative;
       }

       .chartLabel
       {
           width:100%;
           height:100%;
           padding:10px;
           font-size:16px;
           cursor:pointer;
           padding-top:30px;
       }

        .chartBut
       {
          display:none;
          position:absolute;
          left:10px;
          top:10px;
          color:#ffffff;
          font-size:20px;
          cursor:pointer;
       }


       .chartBoxShow
       {
           width:auto;
           left:400px;
       }

       .chartBoxShow .chartBody
       {
           display:block;
       } 
        .chartBoxShow .chartLabel
       {
           display:none;
       }
         .chartBoxShow .chartBut
       {
           display:block;
       }
 

