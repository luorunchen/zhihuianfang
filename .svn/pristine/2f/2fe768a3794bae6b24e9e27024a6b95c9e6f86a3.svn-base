html, body {
    width:100%;
    height:100%;
    margin:0px;
    padding:0px;
    background-color:#f4f6fa;
      font-family: 'Microsoft Yahei',verdana;
}
.ToolBarBox {
    position:absolute;
    top:10px;
    left:10px;
    right:10px;
}
.reportBody {
    position:absolute;
    top:55px;
    left:0px;
    right:0px;
    bottom:0px;
}

.topBox {
    position:absolute;
    left:0px;
    right:0px;
    top:0px;
    height:50%;
}

.bottomBox {
      position:absolute;
    left:0px;
    right:0px;
    bottom:0px;
    height:50%;
}

.chartBox {
    position:absolute;
    left:10px;
    top:0px;
    right:10px;
    bottom:8px;
    background-color:#fff;
    border:solid 1px rgb(221, 221, 221);
    border-radius:5px;
}

 .chartBox .chartdemo {
       width:100%;
       height:100%;
 
    }



.tableBox {
    position:absolute;
    left:10px;
    top:0px;
    right:10px;
    bottom:10px;
    background-color:#fff;
     text-align:center;
     overflow:auto;
}
.tableBox  .tabTitle {
     width:100%;
     text-align:center;
     font-size:18px;
     height:50px;
     line-height:50px;
}
 .tabNewTitle {
     width:100%;
     text-align:center;
     font-size:18px;
     height:50px;
     line-height:50px;
}
.tabdemo {
    margin-left:auto;
    margin-right:auto;
    font-size:14px;
    border-collapse:collapse;
    width:100%;
   
}

    .tabdemo td {
        border:solid 1px  #bce8f1;
        padding:5px;
         text-align:center;
    }

    .tabdemo thead td {
        font-weight:bold;
        text-align:center;
        height:25px;
        line-height:25px;
        vertical-align:middle;
        background-color:#337ab7;
        color:#fff;
    }
    .tabdemo .signtd {
         background-color:#f9f9f9;
         
    }
     .tabdemo .colortd {
         font-weight:bold;
        text-align:center;
        height:25px;
        line-height:25px;
        vertical-align:middle;
        background-color:#337ab7;
        color:#fff;
         
    }
    .tabdemo tbody td {
        height:25px;
         line-height:25px;
        vertical-align:middle;
    }


       .Toolbar_RightBox {
        font-family: 'Microsoft Yahei',verdana;
        position: absolute;
        top: 3px;
        right:20px;
        height:35px;
        width:500px;
    }