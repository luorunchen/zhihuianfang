 .ShowBox {
         position:absolute;top:0px;bottom:40px;right:0px;left:0px;
        border: solid 1px #DFDFDF;
        background-color: #fff;
        border-radius:3px;
        overflow:auto;
        font-family: 'Microsoft Yahei',verdana;
        padding-top:20px;

    }
   .ButsBox {
         position:absolute;bottom:0px;right:0px;left:0px;
         height:40px;
        text-align:right;
        padding-top:5px;
       padding-right:20px;
    }

  .ShowBox  .textTitle {
    width:100%;
    font-size:20px;
    text-align:center;
    line-height:60px;
    font-weight:bold;
}

.ShowTable {
    margin-left:auto;
    margin-right:auto;
    width:650px;
    font-family: 'Microsoft Yahei',verdana;
}
    .ShowTable .tabTitle {
        text-align:center;
        height:40px;
        line-height:40px;
        font-size:20px;
        font-weight:bold;
    }
    .ShowTable .tabSubTitle {
        text-align:center;
        height:30px;
        line-height:30px;
    }

     .ShowTable .tabBox{
        border:none;
         font-size:14px;
          width:100%;
          table-layout:fixed;
          border-collapse:collapse;
          border-top:solid 1px #000;
          border-left:solid 1px #000;
    }
    .alarmTable .tabBox{
        border:none;
         font-size:14px;
          width:100%;
          table-layout:fixed;
          border-collapse:collapse;
          border-top:solid 1px #000;
          border-left:solid 1px #000;
    }
    .facEvent .tabBox{
        border:none;
         font-size:14px;
          width:100%;
          table-layout:fixed;
          border-collapse:collapse;
          border-top:solid 1px #000;
          border-left:solid 1px #000;
    }
    .ShowTable  td {
       border-right:solid 1px #000;
       border-bottom:solid 1px #000;
        line-height:25px;
        min-height:25px;
        padding:5px;
       word-break:break-all;  
        word-wrap:break-word;  
    }
    .alarmTable  td {
       border-right:solid 1px #000;
       border-bottom:solid 1px #000;
        line-height:25px;
        min-height:25px;
        padding:5px;
       word-break:break-all;  
        word-wrap:break-word;  
    }
    .facEvent td {
        border-right:solid 1px #000;
        border-bottom:solid 1px #000;
         line-height:25px;
         min-height:25px;
         padding:5px;
        word-break:break-all;  
         word-wrap:break-word;  
     }
     .ShowTable  thead
     {

          background-color:#f5f5f5;
     }
      .ShowTable  thead td
     {

        text-align:center;
     }
     .ShowTable .tabSection {
        text-align:center;
        vertical-align:middle;
        background-color:#dddddd;
        color:#0000ff;
        font-size:16px;
    }

    .ShowTable .tabLabel {
        text-align:center;
        vertical-align:middle;
        background-color:#f5f5f5;
    }
    .alarmTable .tabLabel {
        text-align:center;
        vertical-align:middle;
        background-color:#f5f5f5;
    }
    .facEvent .tabLabel {
        text-align:center;
        vertical-align:middle;
        background-color:#f5f5f5;
    }

    .ShowTable .tabValue {
        text-align:center;
        vertical-align:middle;
    }


    .ShowTable .ShowImg {
        max-width: 95%;
        height: auto;
        width: auto\9;
    }

    .ShowTable .ShowImgTitle {
        line-height:40px;
        height:40px;
        text-align:center;
    }

      .ShowTable .ShowImgBox {
        width:100%;
        position:relative;
    }
       .chkBox {
       width:100px;
        float:left;
        text-align:center;
    }
    .ChkBH {
        line-height:20px;
        text-align:center;
        width:100%;
        padding:5px;
        word-break:break-all; 
        word-wrap:break-word;
        height:65px;
    }

    .chkPoint {
       margin-left:auto;
       margin-right:auto;
       width:85px;
       height:100px;
       text-align:center;
       color:#fff;
       cursor:pointer;
       padding-left:10px;
       padding-right:10px;

       display: -webkit-box;
      -webkit-box-orient: horizontal;
      -webkit-box-pack: center;
      -webkit-box-align: center;
  
  
  


    }


     .nochange {
        background: url('/content/grid/images/nochange.png') no-repeat;
        background-size:100% 100%;
    }
    .nocheck {
        background: url('/content/grid/images/nocheck.png') no-repeat;
        background-size:100% 100%;
    }
    .yescheck {
        background: url('/content/grid/images/yescheck.png') no-repeat;
        background-size:100% 100%;
    }


