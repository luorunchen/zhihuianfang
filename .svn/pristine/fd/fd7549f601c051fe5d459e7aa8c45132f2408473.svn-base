.showMenu {
    margin-left:auto;
    margin-right:auto;
    font-family: 'Microsoft Yahei',verdana;
    color:#fff;
    margin-bottom:20px;
     width:900px;
}    

    .showMenu .col-xs-4 {
        padding-left:5px;
        padding-right:5px;
    }

    .showMenu .menuItem1 {
        text-align:center;
        width:285px;
        height:65px;
        line-height:65px;
        clear:both;
        font-size:18px;
