        border-radius:3px;
        cursor:pointer;
    }

    .showMenu .bg1 {
        background-color:#e07553;
    }
    .showMenu .bg2 {
        background-color:#2fb6d4;
    }
     .showMenu .bg3 {
        background-color:#f4af24;
    }
    .showMenu .bg4 {
