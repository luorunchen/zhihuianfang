        //     var date=now.getDate();      //返回日期月份中的天数（1到31）
        //     var hour=now.getHours();     //返回日期中的小时数（0到23）
        //     var minute=now.getMinutes(); //返回日期中的分钟数（0到59）
        //     var second=now.getSeconds(); //返回日期中的秒数（0到59）
        //     return year+"-"+month+"-"+date+" "+hour+":"+minute+":"+second; 
        // } 

        $('#daoData').click(function(){
            //http://anfangapp.112.74.126.51/earlyWarn/admin/device/excelAlarmValue.action?imei=00000002000190000300009000300108
            window.open(url+"/admin/device/excelAlarmValue.action?imei="+global.pro);
        })
